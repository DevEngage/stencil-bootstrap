{
  "name": "@dev-engage/stencil-bootstrap",
  "private": false,
  "version": "0.5.4",
  "description": "Stencil's very own Bootstrap implementation",
  "main": "dist/stb.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache",
    "publish": "npm run build && npm publish --access public",
    "deploy": "firebase deploy"
  },
  "dependencies": {
    "@stencil/core": "0.6.18",
    "@stencil/router": "0.0.27",
    "@stencil/sass": "latest",
    "bootstrap": "^4.0.0",
    "popper.js": "^1.14.1"
  },
  "devDependencies": {
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "@types/jest": "^21.1.1",
    "jest": "^21.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevEngage/stencil-bootstrap.git"
  },
  "author": "DevEngage",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DevEngage/stencil-bootstrap"
  },
  "homepage": "https://github.com/DevEngage/stencil-bootstrap",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
